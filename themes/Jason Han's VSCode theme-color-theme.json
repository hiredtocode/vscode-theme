{
	"name": "Jason Han's VSCode theme",
	"colors": {
		"editor.background": "#00293b",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#ffc400",
		"menu.selectionBackground": "#000981",
		"menu.background": "#0d1e2c",
		"activityBarBadge.foreground": "#0011ff",
		"scrollbarSlider.background": "#0051ff3b",
		"scrollbarSlider.hoverBackground": "#0051ff62",
		"scrollbarSlider.activeBackground": "#0051ff33",
		"tab.activeBorderTop": "#006eff",
		"tab.activeForeground": "#ffc400",
		"diffEditor.insertedTextBackground": "#0000002c",
		"editorGroupHeader.tabsBorder": "#00293b",
		"activityBar.background": "#0d1e2c",
		"editorGroupHeader.tabsBackground": "#0d1e2c",
		"panel.background": "#091925",
		"tab.inactiveBackground": "#091925",
		"sideBar.background": "#0f2130",
		"sideBarTitle.foreground": "#bbbbbb",
		"progressBar.background": "#00d9ff",
		"titleBar.activeBackground": "#0d1e2c",
		"terminal.background": "#0d2031",
		"list.inactiveSelectionBackground": "#002f58",
		"input.background": "#00203b",
		"input.foreground": "#80e5ff",
		"editor.findMatchHighlightBackground": "#0036e646",
		"editor.findMatchBorder": "#00e645bd",
		"editor.findMatchBackground": "#acaf0000",
		"list.hoverBackground": "#002981",
		"editorBracketMatch.border": "#ff0000"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6d77ffdc"
			}
		},

		// {
		// 	"name": "Variables",
		// 	"scope": ["variable"],
		// 	"settings": {
		// 		"foreground": "#46caff"
		// 	}
		// },
		{
			"name": "",
			"scope": [
				"meta.objectliteral.js meta.object.member.js variable.other.object.property.js"
			],
			"settings": {
				"foreground": "#70ff53"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color",
				"meta.jsx.children.js.jsx",
				"meta.jsx.children.js"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"variable.other.property.js",
				"variable.other.object.js"
			],
			"settings": {
				"foreground": "#01afff"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",

				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#00d19d"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",

				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#94d8ff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",

				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#90d6ff"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other", "variable.other.readwrite.alias"],
			"settings": {
				"foreground": "#3bc1ff"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#ff2e39"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#46caff"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "template literal text",
			"scope": ["string.template.js"],
			"settings": {
				"foreground": "#C3E88D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"meta.property-value",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types",
				"punctuation"
			],
			"settings": {
				"foreground": "#02e9d6"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name",
				"meta.object-literal.key.js",
				"entity.name.function.member"
			],
			"settings": {
				"foreground": "#ff9900"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js",

				"keyword.operator.ternary.js.jsx",
				"keyword.operator.comparison.js.jsx"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": ["entity.name.method.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#e09c5c"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
